package com.fahri;

import javax.ws.rs.GET;
import javax.ws.rs.Path;

@Path(value="/kullanici")
public class Merkez {
	public static void main(String[] args) {
		
		System.out.println("Merhaba");
		
	}
	
	@GET
	@Path(value="")
	public void karsila(String[] args) {
		
		System.out.println("Selamlar Merhaba");
		
	}
}
